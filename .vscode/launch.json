{
   "version": "1.0.0",
   "configurations": [
      {
         "type": "node",
         "request": "attach",
         "name": "Server debugger",
         "restart": true,
         "port": 9229,
         "sourceMaps": true,
         "skipFiles": ["<node_internals>/**"]
      },
      {
         "type": "chrome",
         "request": "launch",
         "name": "Web debugger",
         "url": "http://localhost:3000",
         "webRoot": "${workspaceFolder}/apps/web/src"
      }
   ]
}
