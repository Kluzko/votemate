{
   "name": "@votemate/server",
   "private": true,
   "version": "1.0.0",
   "scripts": {
      "start": "cross-env NODE_ENV=production node dist/src/server.js",
      "dev": "cross-env NODE_ENV=development nodemon --inspect src/server.ts",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc",
      "check": "pnpm lint && pnpm ts:check",
      "build": "tsc",
      "prisma": "npx prisma generate --schema=src/prisma/schema.prisma",
      "prisma:studio": "npx prisma studio --schema=src/prisma/schema.prisma",
      "prisma:push": "npx prisma db push"
   },
   "dependencies": {
      "@prisma/client": "^4.8.0",
      "cookie-parser": "^1.4.6",
      "express": "^4.18.2",
      "pino": "^8.14.1",
      "pino-pretty": "^10.0.0",
      "zod": "^3.19.1"
   },
   "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@swc/core": "^1.3.8",
      "@votemate/eslint-config": "*",
      "@votemate/tsconfig": "*",
      "@types/cookie-parser": "^1.4.3",
      "@types/express": "^4.17.14",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.11.9",
      "cross-env": "^7.0.3",
      "dotenv": "^16.0.3",
      "module-alias": "^2.2.2",
      "nodemon": "^2.0.20",
      "prisma": "4.8.0",
      "ts-node": "^10.9.1",
      "typescript": "^4.9.3"
   }
}
