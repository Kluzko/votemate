{
   "name": "@votemate/server",
   "private": true,
   "version": "1.0.0",
   "scripts": {
      "start": "cross-env NODE_ENV=production node dist/src/server.js",
      "dev": "cross-env NODE_ENV=development nodemon --inspect src/server.ts",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc",
      "check": "pnpm lint && pnpm ts:check",
      "test:unit": "vitest .unit.test.ts",
      "test:integration": "vitest integration.test.ts",
      "test:e2e": "vitest .e2e.test.ts --single-thread",
      "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
      "build": "tsc",
      "prisma": "npx prisma generate --schema=src/prisma/schema.prisma",
      "prisma:studio": "npx prisma studio --schema=src/prisma/schema.prisma",
      "prisma:push": "npx prisma db push --schema=src/prisma/schema.prisma"
   },
   "dependencies": {
      "@fastify/cookie": "^8.3.0",
      "@prisma/client": "^4.8.0",
      "cookie-parser": "^1.4.6",
      "fastify": "^4.18.0",
      "inversify": "^6.0.1",
      "jsonwebtoken": "^9.0.0",
      "pino": "^8.14.1",
      "pino-pretty": "^10.0.0",
      "reflect-metadata": "^0.1.13",
      "resend": "^0.16.0",
      "socket.io": "^4.7.1",
      "uuid": "^9.0.0",
      "zod": "^3.19.1"
   },
   "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@swc/core": "^1.3.8",
      "@trivago/prettier-plugin-sort-imports": "^4.1.1",
      "@types/cookie-parser": "^1.4.3",
      "@types/jsonwebtoken": "^9.0.2",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.11.9",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.2",
      "@votemate/eslint-config": "workspace:*",
      "@votemate/tsconfig": "workspace:*",
      "cross-env": "^7.0.3",
      "dotenv": "^16.0.3",
      "module-alias": "^2.2.2",
      "nodemon": "^2.0.20",
      "prisma": "4.8.0",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "typescript": "^4.9.3",
      "vite-tsconfig-paths": "^4.2.0",
      "vitest": "^0.32.2"
   }
}
